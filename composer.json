{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "algolia/algoliasearch-client-php": "^3.2",
        "algolia/scout-extended": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "blade-ui-kit/blade-heroicons": "^2.0",
        "davidpiesse/nova-toggle": "^0.0.8",
        "doctrine/dbal": "^3.3",
        "drobee/nova-sluggable": "^1.2",
        "gabrieliuga/laravel-nova-sidebar-links": "^1.0",
        "genealabs/nova-gutenberg": "^0.2.0",
        "genert/bbcode": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "inspheric/nova-indicator-field": "^1.43",
        "jbroadway/urlify": "dev-voku-portable-ascii-2.0",
        "kabbouchi/nova-impersonate": "^1.11",
        "kiritokatklian/nova-permission": "^3.0",
        "laravel/cashier": "^13.10",
        "laravel/framework": "^9.12",
        "laravel/helpers": "^1.5",
        "laravel/horizon": "^5.9",
        "laravel/jetstream": "^2.4",
        "laravel/nova": "^3.31",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "limedeck/nova-cashier-overview": "^3.0",
        "livewire/livewire": "^2.3",
        "maatwebsite/excel": "^3.1",
        "mydnic/laravel-kustomer": "^1.6",
        "mydnic/nova-kustomer": "^1.3",
        "outl1ne/nova-menu-builder": "^6.0",
        "owenvoke/blade-fontawesome": "^2.0",
        "propaganistas/laravel-phone": "^4.3",
        "spatie/laravel-mailcoach": "^5.0",
        "spatie/laravel-mailcoach-mailgun-feedback": "^4.0",
        "spatie/laravel-mailcoach-unlayer": "^2.0",
        "spatie/laravel-menu": "^4.1",
        "spatie/laravel-permission": "^5.0",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-tags": "^4.3",
        "stevebauman/purify": "^4.0",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "fakerphp/faker": "^1.9.1",
        "spatie/laravel-ignition": "^1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "@php artisan nova:publish",
            "@php artisan vendor:publish --tag mailcoach-assets --force",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
